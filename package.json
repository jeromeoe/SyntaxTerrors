    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "express": "^4.18.3",
    "cors": "^2.8.5",
    "http-proxy-middleware": "^2.0.6",
    "concurrently": "^8.2.2",
    "crypto": "^1.0.1",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.5",
